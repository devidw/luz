		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@unocss/svelte-scoped": "66.1.0-beta.10",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"socket.io-client": "^4.8.1",
		"svelte": "^5.0.0",
		"svelte-autosize": "^1.1.5",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
